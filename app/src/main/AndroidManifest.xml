            </intent-filter>
            </activity>
    </application>

    <instrumentation
        android:name="com.google.android.apps.common.testing.testrunner.GoogleInstrumentationTestRunner"
        android:targetPackage="com.protegra.dsparling.testbed"
        android:label="Espresso tests for com.protegra.dsparling.testbed"/>

